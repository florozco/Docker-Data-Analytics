# Auto detect text files and perform LF normalization
* filter=lfs diff=lfs merge=lfs -text
*.map filter=lfs diff=lfs merge=lfs -text
*.init filter=lfs diff=lfs merge=lfs -text
*.stat filter=lfs diff=lfs merge=lfs -text
*.conf filter=lfs diff=lfs merge=lfs -text
*.opts filter=lfs diff=lfs merge=lfs -text
postgres-db/**/* filter=lfs diff=lfs merge=lfs -text
